Windows Registry Editor Version 5.00

[HKEY_CURRENT_USER\Software\Classes\.ps1]
@="PowerShellScript"

[HKEY_CURRENT_USER\Software\Classes\PowerShellScript]
@="PowerShell Script"

[HKEY_CURRENT_USER\Software\Classes\PowerShellScript\shell]

[HKEY_CURRENT_USER\Software\Classes\PowerShellScript\shell\open]

